server {
    listen localhost:8008;
    max_body_size 2G;
    allow GET DELETE POST;
    root /Users/zmakhkha/Desktop;
    upload_path /Users/zmakhkha/goinfre/up/;
    location / {
        root /Users/zmakhkha/Desktop;
        autoindex ON;
    }
    location /py {
        root /Users/zmakhkha/Desktop/WebServer;
        allow GET POST;
        autoindex ON;
        cgi py /usr/bin/python;
    }
    location /php {
        root /Users/zmakhkha/Desktop/WebServer/public/scripts/php;
        allow GET POST;
        autoindex ON;
        cgi php /Users/zmakhkha/Desktop/WebServer/cgi-bin/php-cgi;
    }
}
